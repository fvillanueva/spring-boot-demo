/****************************************
  Name: Table Articles
  Description: Table of all content articles
  Created: 13-08-2017
  Author: Fidel Villanueva Delgado
 */
create table ARTICLES
(
	ARTICLE_ID int(5) auto_increment
		primary key,
	TITLE varchar(200) not null,
	CATEGORY varchar(100) not null
);

